generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id           String @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
    username     String @unique
    passwordHash String
}
